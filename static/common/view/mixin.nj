
{% set vertion = __webpack__.hash %}
{% macro script(list,v= __webpack__.hash ) %}
    {% for item in list %}
        <script src='<$ item $>?v=<$ v $>' type='text/javascript'></script>
    {% endfor %}
{% endmacro %}

{% macro style(list) %}
    {% for item in list %}
        <link rel="stylesheet" type="text/css" href='<$ item $>?v=<$ vertion $>' />
    {% endfor %}
{% endmacro %}

{% block html %}{% endblock %}